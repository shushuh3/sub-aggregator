# Копия .env для примера. НЕ содержит реальных паролей!
# Скопируйте в .env и заполните реальными значениями

ENV=development

# Database Configuration (ЧУВСТВИТЕЛЬНЫЕ ДАННЫЕ - хранить только в .env!)
DB_HOST=localhost
DB_PORT=5432
DB_USER=your_db_user
DB_PASSWORD=your_secure_password
DB_NAME=subscriptions_db
DB_SSLMODE=disable

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8080
SERVER_READ_TIMEOUT=10s
SERVER_WRITE_TIMEOUT=10s

# Logger Configuration
LOG_LEVEL=info
LOG_FORMAT=json
